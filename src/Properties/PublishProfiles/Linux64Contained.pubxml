<Project>
	<PropertyGroup>
		<Platform>Any CPU</Platform>
		<PublishProtocol>FileSystem</PublishProtocol>
		<_TargetId>Folder</_TargetId>
		<RuntimeIdentifier>linux-x64</RuntimeIdentifier>
		<SelfContained>true</SelfContained>
		<PublishTrimmed>false</PublishTrimmed>
		<PublishDir Condition="'$(SelfContained)'=='true'">bin\$(Configuration)\$(TargetFramework)\Publish\$(RuntimeIdentifier)\packed</PublishDir>
		<PublishDir Condition="'$(SelfContained)'=='false'">bin\$(Configuration)\$(TargetFramework)\Publish\$(RuntimeIdentifier)\min</PublishDir>
	</PropertyGroup>
</Project>